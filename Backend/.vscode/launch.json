{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "name": "Backend",
            "request": "launch",
            "program": "${workspaceFolder}/bin/index.js",
            "cwd": "${workspaceRoot}",
            "args": [
                "DEBUG"
            ],
            "envFile": "${workspaceFolder}/config.env",
            "env": {},
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "smartStep": true,
            "skipFiles": [
                "<node_internals>/**"
            ],
        }
    ]
}